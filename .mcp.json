{
  "mcpServers": {
    "github": {
      "type": "stdio",
      "command": "cmd",
      "args": [
        "/c",
        "npx -y dotenv-cli -- npx -y @modelcontextprotocol/server-github"
      ],
      "env": {
        "DOTENV_CONFIG_PATH": ".mcp.env.local"
      }
    },
    "context7": {
      "type": "stdio",
      "command": "cmd",
      "args": [
        "/c",
        "npx -y dotenv-cli -- npx -y @upstash/context7-mcp"
      ],
      "env": {
        "DOTENV_CONFIG_PATH": ".mcp.env.local"
      }
    },
    "chrome-devtools": {
      "type": "stdio",
      "command": "cmd",
      "args": [
        "/c",
        "npx -y chrome-devtools-mcp@latest"
      ],
      "env": {}
    },
    "playwright": {
      "type": "stdio",
      "command": "cmd",
      "args": [
        "/c",
        "npx -y dotenv-cli -- npx --extension  @playwright/mcp@latest --extension"
      ],
      "env": {
        "DOTENV_CONFIG_PATH": ".mcp.env.local"
      }
    }
  }
}