{
  "enabledMcpjsonServers": [
    "github",
    "context7",
    "chrome-devtools",
    "playwright"
  ],
  "enableAllProjectMcpServers": true,
  "permissions": {
    "allow": [
      "mcp__chrome-devtools__*",
      "mcp__github__*",
      "mcp__context7__*",
      "mcp__playwright__*",
      "mcp__web-search-prime__*",
      "mcp__web-reader__*",
      "mcp__zai-mcp-server__*",
      "mcp__ide__*",
      "Bash(cp:*)",
      "Bash(mkdir:*)",
      "Bash(npm:*)",
      "Bash(npx:*)",
      "Bash(cd:${workspaceFolder}/*)"
    ],
    "deny": []
  }
}
